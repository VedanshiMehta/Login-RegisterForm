<?xml version="1.0" encoding="utf-8" ?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

	<item android:state_enabled="true" 
		android:state_pressed="true" >

		<shape  android:shape="rectangle">

			<padding android:left="@dimen/size5"
					  android:right="@dimen/size5"
					  android:top="@dimen/size5"
					  android:bottom="@dimen/size5" />

			<corners android:radius="@dimen/size20"/>

			<solid android:color="@color/oxfordBlue" />

			<stroke android:color="@color/oxfordBlue"
					android:width="@dimen/size2"/>
		</shape>
	</item>
	<item android:state_enabled="true" android:state_pressed="false" >


		<shape  android:shape="rectangle">

			<padding android:left="@dimen/size5"
						android:right="@dimen/size5"
						android:top="@dimen/size5"
						android:bottom="@dimen/size5" />

			<corners android:radius="@dimen/size20"/>

			<solid android:color="@color/vividBlue" />

			<stroke android:color="@color/vividBlue"
					android:width="@dimen/size2"/>
		</shape>
	</item>
	




</selector>




			
	
	

	
